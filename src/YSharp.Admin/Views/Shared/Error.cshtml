
@{
    Layout = "~/Views/Shared/_Layout_Login.cshtml";
}

<div style="padding-top:10%">
    <div style="text-align:center">
        <img src="/img/logo.png" style="height:58px;width:auto;">

    </div>
    <div style="text-align:center">
        <h2 style="color:red">
            Error
        </h2>
    </div>
    <div class="middle-box   animated fadeInDown " style="padding-top:20px">
        <div class="ibox-content" style="padding:55px 35px 85px 35px;">
            <div class="form-group">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            </div>
            <div class="form-group">

            </div>
            <div class="form-group">
                <p style="color:red">
                    An error occurred while processing your request.
                </p>
            </div>
            <div class="form-footer" style="margin-top:50px">
                <a href="javascript:history.back();" class="btn btn-default btn-block">
                    Back to try again
                </a>
            </div>
        </div>
    </div>
</div>
